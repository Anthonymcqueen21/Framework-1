{
    "name": "ucsdmath/framework",
    "type": "library",
    "description": "A top controller, event listener, and router for application testing",
    "keywords": ["framework", "ucsdmath", "event", "core", "router", "dispatcher", "httpkernel", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-03-01",
    "authors": [
        {"name": "Daryl Eisner", "email": "deisner@ucsd.edu"}
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Framework",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": "^7.0",
        "symfony/routing": "3.0.4",
        "symfony/http-kernel": "3.0.4",
        "symfony/http-foundation": "3.0.4",
        "symfony/event-dispatcher": "3.0.4",
        "ucsdmath/dependency-injection": "1.7.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.4",
        "phpunit/phpunit": "5.3.0",
        "ucsdmath/testing": "1.7.x-dev",
        "squizlabs/php_codesniffer": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Framework\\": "src/Framework/",
            "UCSDMath\\Framework\\Core\\": "src/Framework/Core/",
            "UCSDMath\\Framework\\Events\\": "src/Framework/Event/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    }
}

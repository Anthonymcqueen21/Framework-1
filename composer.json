{
    "name": "ucsdmath/framework",
    "type": "library",
    "description": "A top controller, event listener, and router for application testing",
    "keywords": ["framework", "ucsdmath", "event", "core", "router", "dispatcher", "httpkernel", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-07-03",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/routing": "3.1.2",
        "symfony/http-kernel": "3.1.2",
        "symfony/http-foundation": "3.1.2",
        "symfony/event-dispatcher": "3.1.2",
        "symfony/framework-bundle": "3.1.2",
        "ucsdmath/configuration": "1.8.x-dev",
        "ucsdmath/dependency-injection": "1.8.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.5",
        "phpunit/phpunit": "5.4.7",
        "ucsdmath/testing": "1.8.x-dev",
        "squizlabs/php_codesniffer": "2.6.2"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Framework\\": "src/Framework/",
            "UCSDMath\\Framework\\Core\\": "src/Framework/Core/",
            "UCSDMath\\Framework\\Events\\": "src/Framework/Event/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    },
    "minimum-stability": "dev"
}

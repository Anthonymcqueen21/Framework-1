{
    "name": "ucsdmath/framework",
    "type": "library",
    "description": "A top controller, event listener, and router for application testing.",
    "keywords": ["framework", "ucsdmath", "event", "core", "router", "dispatcher", "httpkernel", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-12-03",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/routing": "4.0.0",
        "symfony/http-kernel": "4.0.0",
        "symfony/http-foundation": "4.0.0",
        "symfony/event-dispatcher": "4.0.0",
        "symfony/framework-bundle": "4.0.0",
        "ucsdmath/configuration": "2.0.0",
        "ucsdmath/dependency-injection": "2.0.0"
    },
    "require-dev": {
        "symfony/debug": "4.0.0",
        "mockery/mockery": "1.0.0",
        "phpunit/phpunit": "6.5.1",
        "fzaninotto/faker": "1.7.1",
        "ucsdmath/date": "2.0.0",
        "symfony/var-dumper": "4.0.0",
        "symfony/browser-kit": "4.0.0",
        "symfony/dom-crawler": "4.0.0",
        "symfony/options-resolver": "4.0.0",
        "squizlabs/php_codesniffer": "3.1.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Framework\\": "src/Framework/",
            "UCSDMath\\Framework\\Core\\": "src/Framework/Core/",
            "UCSDMath\\Framework\\Events\\": "src/Framework/Event/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev"
}

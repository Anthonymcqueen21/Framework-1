{
    "name": "ucsdmath/framework",
    "description": "A top controller, event listener, and router for application testing",
    "keywords": ["core", "event", "router", "dispatcher", "httpkernel", "ucsdmath"],
    "homepage": "https://github.com/UCSDMath",
    "type": "library",
    "license": "MIT",
    "time": "2015-12-05",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Framework",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": ">=5.6.10",
        "symfony/routing": "3.0.*",
        "symfony/http-kernel": "3.0.*",
        "symfony/http-foundation": "3.0.*",
        "symfony/event-dispatcher": "3.0.*",
        "ucsdmath/dependency-injection": "1.6.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "5.1.*",
        "squizlabs/php_codesniffer": "2.4.*"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Framework\\": "src/Framework/",
            "UCSDMath\\Framework\\Core\\": "src/Framework/Core/",
            "UCSDMath\\Framework\\Events\\": "src/Framework/Event/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}